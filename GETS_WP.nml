//##########################
//###Template for Sprites###				
//##########################

template template_waypoint_1x (x,y) {
    [   0, 0, 64, 80, -32,   -50 ]
    [  72, 0, 64, 80, -32,   -50 ] }

template template_waypoint_2x (x,y) {
    [   0, 0, 128, 160, -64,  -100]
    [ 144, 0, 128, 160, -64,  -100] }
				
template template_waypoint_4x (x,y) {
    [   0, 0, 256, 320, -128, -200]
    [ 288, 0, 256, 320, -128, -200] }


//Only used for Reiterstellwerk    
    
template template_STW_Faller1_1x (x,y) {
    [   0, 0, 64, 80, -56,   -62]
    [  72, 0, 64, 80,  -8,   -62] }

template template_STW_Faller1_2x (x,y) {
    [   0, 0, 128, 160, -112, -124]
    [ 144, 0, 128, 160, -16,  -124] }
				
template template_STW_Faller1_4x (x,y) {
    [   0, 0, 256, 320, -224, -248]
    [ 288, 0, 256, 320,  -32, -248] }     


//#################
//###Trapeztafel###				
//#################



spriteset(spriteset_Trapeztafel_L, "gfx/Trapeztafel_L/Trapeztafel_L_1x_8bpp.png")
									{ template_waypoint_1x(0, 0)}
				
	alternative_sprites (spriteset_Trapeztafel_L, ZOOM_LEVEL_NORMAL, BIT_DEPTH_32BPP, "gfx/Trapeztafel_L/Trapeztafel_L_1x_32bpp.png", "gfx/Trapeztafel_L/Trapeztafel_L_1x_mask.png")
									{ template_waypoint_1x(0, 0)}
				
	alternative_sprites (spriteset_Trapeztafel_L, ZOOM_LEVEL_IN_2X, BIT_DEPTH_32BPP, "gfx/Trapeztafel_L/Trapeztafel_L_2x_32bpp.png", "gfx/Trapeztafel_L/Trapeztafel_L_2x_mask.png")
									{ template_waypoint_2x(0, 0)}
				
	alternative_sprites (spriteset_Trapeztafel_L, ZOOM_LEVEL_IN_4X, BIT_DEPTH_32BPP, "gfx/Trapeztafel_L/Trapeztafel_L_4x_32bpp.png", "gfx/Trapeztafel_L/Trapeztafel_L_4x_mask.png")
									{ template_waypoint_4x(0, 0)}

spritelayout Trapeztafel_L_X(a) {
    ground {
        sprite: 1012 + a; // prevent railtype offset
    }
    building {
        sprite: spriteset_Trapeztafel_L (0); // first sprite in active spriteset
        xoffset: 0;
        yoffset: 12;
        zoffset: 0;        
        xextent: 16;
        yextent: 4;
        zextent: 4;
        recolour_mode: RECOLOUR_REMAP;
        palette:       PALETTE_USE_DEFAULT;
    }
}

spritelayout Trapeztafel_L_Y(a) {
    ground {
        sprite: 1011 + a; // prevent railtype offset
    }
    building {
        sprite: spriteset_Trapeztafel_L(1); // second sprite in active spriteset
        xoffset: 12;
        yoffset: 0;
        zoffset: 0;        
        xextent: 4;
        yextent: 16;
        zextent: 4;
        recolour_mode: RECOLOUR_REMAP;
        palette:       PALETTE_USE_DEFAULT;
    }
}

item(FEAT_STATIONS, Trapeztafel_L) {
    property {
        /* The class allows to sort stations into categories. */
        class:                  "WAYP";
        /* If no other NewGRF provides this class before us, we have to name it */
        classname:              string(STR_NAME_STATCLASS);
        /* Name of this particular station */
        name:                   string(STR_NAME_STATION);
        draw_pylon_tiles: 0;
        hide_wire_tiles: 0xFF;
        
    }
    graphics {
        sprite_layouts: [Trapeztafel_L_X(0), Trapeztafel_L_Y(0)];
        purchase: spriteset_Trapeztafel_L;
        
    }
}


